<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="less12"
          fetch="https://github.com/lessaosp"
          revision="snow" />

  <remote name="less-devices"
          fetch="https://github.com/Lessaosp-Devices"
          revision="snow" />

  <!-- General repos -->
  <project path="build/make" name="build" remote="less12">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="build_soong" remote="less12" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  <project path="manifest" name="manifest" remote="less12" />
  <project path="vendor/aosp" name="vendor_aosp" remote="less12" />

  <!-- Framework repos -->
  <project path="frameworks/base" name="frameworks_base" remote="less12" />

  <!-- Packages repos-->
  <project path="packages/apps/Settings" name="packages_apps_Settings" remote="less12" />

</manifest>